# Chess Notation OCR Lambda Environment Variables
# Copy this file to .env and update with your AWS resource names

# S3 bucket name for storing uploaded chess notation images
S3_BUCKET_NAME=chess-notation-uploads

# DynamoDB table name for tracking workflow state
DYNAMODB_TABLE_NAME=ChessNotationOCRWorkflows

# AWS region where resources are deployed
AWS_REGION=us-east-1

# Lambda function name (used by deployment script)
FUNCTION_NAME=ChessNotationOCR

# IAM role name for Lambda execution (created by infrastructure script)
ROLE_NAME=ChessNotationOCRLambdaRole
